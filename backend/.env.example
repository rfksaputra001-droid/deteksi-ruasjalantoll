# Server Configuration
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000
CLIENT_URL=http://localhost:3001

# MongoDB
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/yolo-detection?retryWrites=true&w=majority

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-min-32-chars
JWT_EXPIRE=7d

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
CLOUDINARY_FOLDER=yolo-videos

# Video Processing
MAX_VIDEO_SIZE=5368709120
VIDEO_EXPIRY_DAYS=7
VIDEO_QUALITY=480p
ALLOWED_VIDEO_FORMATS=mp4,avi,mov,mkv

# Admin Default Credentials
DEFAULT_ADMIN_EMAIL=admin@yolo.com
DEFAULT_ADMIN_PASSWORD=Admin123!
DEFAULT_ADMIN_NAME=Super Admin
