# Server Configuration
NODE_ENV=production
PORT=10000
API_BASE_URL=https://your-app-name.onrender.com
CLIENT_URL=https://your-frontend-domain.com

# MongoDB
MONGODB_URI=your-mongodb-atlas-connection-string

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-min-32-chars
JWT_EXPIRE=7d

# Cloudinary
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
CLOUDINARY_FOLDER=yolo-deteksi

# Video Processing
MAX_VIDEO_SIZE=5368709120
ALLOWED_FILE_TYPES=video/mp4,video/avi,video/mov,video/wmv,video/mkv

# File Upload Limits
MAX_FILE_SIZE=5368709120
UPLOAD_TIMEOUT=600000

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Python
PYTHON_PATH=python
YOLO_SCRIPT_PATH=./src/models/vehicle-night-yolo/detect.py

# Cors
CORS_ORIGIN=*

# Socket.IO
SOCKET_IO_CORS_ORIGIN=*